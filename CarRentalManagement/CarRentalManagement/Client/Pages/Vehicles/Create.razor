@attribute [Route("/" + PagePath.Vehicles + "/create")]
@inject HttpClient _client
@inject NavigationManager _navManager

<FormComponent ButtonText="Create Vehicle" OnValidSubmit="CreateVehicle" Title="Create New Vehicle" Vehicle="Vehicle" />

@code {
    Vehicle Vehicle = new Vehicle();
    
    private async Task CreateVehicle()
    {
        await _client.PostAsJsonAsync(ApiEndpoints.Vehicles, Vehicle);
        _navManager.NavigateTo(PagePath.Vehicles);
    }

}
